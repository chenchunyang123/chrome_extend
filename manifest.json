{
  "name": "复制插件",
  "version": "1.0",
  "description": "可复制代码，并去掉版权信息",
  "manifest_version": 3,
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["./custom.css"],
      "js": ["./custom.js"],
      "run_at": "document_end"
    }
  ]
}
